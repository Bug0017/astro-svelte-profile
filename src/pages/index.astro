---
  // @ts-ignore
	import BaseLayout from "../layouts/base.layout.svelte";
  // @ts-ignore
  import Comma from "../components/comma.svelte";
---
<BaseLayout title="Bug | Surendar Kalyanam">
  <div class=" w-screen h-screen overflow-x-hidden">
    <div class="p-10">
      <h1 class="text-3xl sure">Bug</h1>
    </div>
    <div class="flex p-20 box-border">
      <div class=" flex flex-col text-8xl sure-i w-1/2">
        <span>
